        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.JoinActivity" />
        <activity android:name=".login.JoinActivity2" />
        <activity android:name=".Main.MainActivity" />
        <activity android:name=".Main.HouseInfoActivity" />
        <activity android:name=".Main.RegisterActivity" />
        <activity android:name=".mypage.MypageActivity"></activity>
    </application>

</manifest>